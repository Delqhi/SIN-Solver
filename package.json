{
  "name": "delqhi-platform-root",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev dashboard -p 3000",
    "build": "next build dashboard && npm run build:workers",
    "build:workers": "cd workers/2captcha-worker && npm run build",
    "build:modules": "cd modules/deception-core && npm run build",
    "start": "next start dashboard -p 3000",
    "lint": "next lint dashboard && npm run lint:workers",
    "lint:workers": "cd workers/2captcha-worker && npm run lint",
    "lint:fix": "npm run lint:fix:dashboard && npm run lint:fix:workers",
    "lint:fix:dashboard": "next lint dashboard --fix",
    "lint:fix:workers": "cd workers/2captcha-worker && eslint src --ext .ts --fix",
    "test": "npm run test:python && npm run test:typescript",
    "test:python": "bash run_tests.sh",
    "test:typescript": "npm run test:workers && npm run test:modules",
    "test:workers": "cd workers/2captcha-worker && npm run test",
    "test:workers:watch": "cd workers/2captcha-worker && npm run test:watch",
    "test:workers:coverage": "cd workers/2captcha-worker && npm run test:coverage",
    "test:modules": "cd modules/deception-core && npm run test",
    "test:all": "npm run test:python && npm run test:typescript && npm run test:e2e",
    "test:e2e": "cd workers/2captcha-worker && npm run test:e2e",
    "typecheck": "tsc --noEmit",
    "typecheck:workers": "cd workers/2captcha-worker && npx tsc --noEmit",
    "typecheck:modules": "cd modules/deception-core && npx tsc --noEmit",
    "typecheck:all": "npm run typecheck:workers && npm run typecheck:modules"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.13.4",
    "clsx": "^2.1.1",
    "dockerode": "^4.0.9",
    "express": "^5.2.1",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.474.0",
    "next": "14.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.1",
    "reactflow": "^11.10.4",
    "recharts": "^3.7.0",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  }
}
