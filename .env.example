# SIN-Solver Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git - it contains sensitive information!

# ==========================================
# DATABASE
# ==========================================
DATABASE_URL=postgresql://username:password@localhost:5432/sin_solver
POSTGRES_USER=sin_solver
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=sin_solver

# ==========================================
# REDIS CACHE
# ==========================================
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=your_redis_password_here

# ==========================================
# SUPABASE (Room-16)
# ==========================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# ==========================================
# N8N ORCHESTRATOR (Agent-01)
# ==========================================
N8N_USER=admin
N8N_PASSWORD=your_n8n_password_here
N8N_WEBHOOK_URL=https://n8n.delqhi.com/webhook

# ==========================================
# AI SERVICE API KEYS
# ==========================================
MISTRAL_API_KEY=your_mistral_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
OPENCODE_ZEN_API_KEY=your_opencode_zen_key_here

# ==========================================
# BROWSER AUTOMATION
# ==========================================
STEEL_API_KEY=your_steel_api_key_here
SKYVERN_API_KEY=your_skyvern_api_key_here

# ==========================================
# AGENT ZERO
# ==========================================
AGENTZERO_API_KEY=your_agentzero_api_key_here

# ==========================================
# PAYMENT PROCESSING (Optional)
# ==========================================
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret

KLARNA_API_KEY=your_klarna_api_key

# ==========================================
# EMAIL SERVICE (Optional)
# ==========================================
GMAIL_EMAIL=your-email@gmail.com
GMAIL_APP_PASSWORD=your_app_password

# ==========================================
# SOCIAL MEDIA APIs (Optional)
# ==========================================
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_access_token
TWITTER_ACCESS_SECRET=your_access_secret

INSTAGRAM_USERNAME=your_instagram_username
INSTAGRAM_PASSWORD=your_instagram_password

TIKTOK_ACCESS_TOKEN=your_tiktok_access_token

WHATSAPP_PHONE_ID=your_whatsapp_phone_id
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token

# ==========================================
# MARKETPLACE APIs (Optional)
# ==========================================
EBAY_APP_ID=your_ebay_app_id
EBAY_CERT_ID=your_ebay_cert_id
EBAY_AUTH_TOKEN=your_ebay_auth_token
EBAY_KA_USERNAME=your_ebay_username
EBAY_KA_PASSWORD=your_ebay_password

# ==========================================
# CLOUDFLARE TUNNEL
# ==========================================
CLOUDFLARE_TUNNEL_TOKEN=your_cloudflare_tunnel_token

# ==========================================
# SECURITY
# ==========================================
JWT_SECRET=your_jwt_secret_key_here_min_32_chars
ENCRYPTION_KEY=your_encryption_key_here

# ==========================================
# FEATURE FLAGS
# ==========================================
NEXT_PUBLIC_ENABLE_AI_CHAT=true
NEXT_PUBLIC_ENABLE_DOCKER_CONTROL=true
NEXT_PUBLIC_ENABLE_WORKER_MISSIONS=true

# ==========================================
# ENVIRONMENT
# ==========================================
NODE_ENV=production
NEXT_PUBLIC_ENVIRONMENT=production
NEXT_PUBLIC_API_URL=https://api.delqhi.com
NEXT_PUBLIC_CODESERVER_API_URL=https://codeserver.delqhi.com
