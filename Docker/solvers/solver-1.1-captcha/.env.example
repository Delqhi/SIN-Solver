# Solver 1.1: Captcha Worker
# ===========================

# Database Configuration
DB_USER=sinuser
DB_PASSWORD=change-me-in-production
DB_NAME=sin_solver

# Logging
LOG_LEVEL=info

# Captcha Solving Configuration
SOLVER_1_1_MAX_CONCURRENT=10
SOLVER_1_1_TIMEOUT=15

# OCR Model Configuration
OCR_MODEL=ddddocr
OCR_CONFIDENCE_THRESHOLD=0.8

# Vision Fallback (for complex captchas)
VISION_FALLBACK=gemini
GEMINI_API_KEY=your-gemini-api-key-here

# Supported Captcha Types
SOLVE_TEXT_CAPTCHA=true
SOLVE_SLIDER_CAPTCHA=true
SOLVE_CLICK_CAPTCHA=true
SOLVE_HCAPTCHA=true
SOLVE_RECAPTCHA=true

# Performance Tuning
BATCH_SIZE=5
PARALLEL_SOLVES=3
MODEL_CACHE_ENABLED=true
